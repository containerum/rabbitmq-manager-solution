{
  "env": {
    "RABBITMQ_IMAGE": "rabbitmq:3-management",
    "RABBITMQ_PORT": "15672",
    "RABBITMQ_DEPLOY_NAME": "solution-rabbitmq",
    "RABBITMQ_SVC_NAME": "solution-rabbitmq-svc",
    "RABBITMQ_USER": "admin",
    "RABBITMQDATA": "/var/lib/rabbitmq"
  },
  "compile":{
     "RABBITMQ_DEPLOY_HASH": "$RANDOM_5$",
     "RABBITMQ_SVC_HASH": "$RANDOM_5$",
     "RABBITMQ_PASSWORD": "$RANDOM_8$"
  },
  "run":[
    {
      "type": "deployment",
      "config_file": "deploy-rabbitmq.json"
    },
    {
      "type": "service",
      "config_file": "service-rabbitmq.json"
    }  
  ]
}
